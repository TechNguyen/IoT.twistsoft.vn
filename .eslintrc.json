{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "indent": [
            "error",
            "tab"
        ],
        "no-unused-vars": "off",
        "jsx-quotes": [
            "error",
            "prefer-single"
        ],
        "react/jsx-key": "off",
        "react/display-name": "off",
        "react/prop-types": "off",

        
    }


}